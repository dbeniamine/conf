# Reset
Color_Off='\e[0m'       # Text Reset

# Regular Colors
Black='\e[01;30m'        # Black
Red='\e[01;31m'          # Red
Green='\e[01;32m'        # Green
Yellow='\e[01;33m'       # Yellow
Blue='\e[01;34m'         # Blue
Purple='\e[01;35m'       # Purple
Cyan='\e[01;36m'         # Cyan
White='\e[01;37m'        # White

